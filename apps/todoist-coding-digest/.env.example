# Todoist API token (get from https://todoist.com/app/settings/integrations/developer)
TODOIST_API_TOKEN=your_todoist_api_token_here

# Email recipient for the digest
EMAIL_RECIPIENT=your.email@example.com

# SES verified sender email
SES_SENDER_EMAIL=your.verified@example.com

# Lambda Function URL (set after deploying Lambda with Function URL enabled)
FUNCTION_URL=https://your-lambda-function-url.lambda-url.us-east-1.on.aws/
