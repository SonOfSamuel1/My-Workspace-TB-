# GitHub Trending Report Configuration
# Copy this file to .env and fill in your values

# Email recipient for the weekly trending report
GITHUB_TRENDING_EMAIL=your_email@example.com

# AWS SES Configuration
SES_SENDER_EMAIL=brandonhome.appdev@gmail.com
AWS_REGION=us-east-1

# Optional: GitHub personal access token for higher rate limits
# Without token: 10 requests/min, With token: 30 requests/min
# GITHUB_TOKEN=ghp_your_token_here

# Optional: AWS credentials (not needed if using IAM role or AWS CLI configured)
# AWS_ACCESS_KEY_ID=your_access_key
# AWS_SECRET_ACCESS_KEY=your_secret_key

# Environment (development, staging, production)
ENVIRONMENT=development
