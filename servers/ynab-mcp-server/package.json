{
  "name": "ynab-mcp-server",
  "version": "1.0.0",
  "description": "MCP server for YNAB (You Need A Budget) API integration",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "ynab-mcp-server": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "start": "node dist/index.js",
